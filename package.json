{
  "name": "@esri/calcite-components",
  "version": "1.0.0-beta.36",
  "description": "Web Components for Esri's Calcite Design System.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "unpkg": "dist/calcite/calcite.js",
  "collection:main": "dist/collection/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "hydrate/"
  ],
  "scripts": {
    "build": "npm run copy-icons && stencil build",
    "postbuild": "cpy dist/custom-elements/index.js dist/custom-elements/ --rename index.mjs",
    "copy-icons": "cpy ./node_modules/@esri/calcite-ui-icons/js/*.json ./src/components/calcite-icon/assets",
    "deps:update": "updtr --ex @types/jest @types/puppeteer jest jest-cli puppeteer && git add package*.json && git commit -q -m \"build(deps): bump versions\"",
    "start": "npm run copy-icons && stencil build --dev --watch --serve",
    "test": "npm run copy-icons && stencil test --spec --e2e",
    "test.watch": "npm run copy-icons && stencil test --spec --e2e --watchAll",
    "posttest": "stencil build --prerender",
    "prettier": "npm-run-all --parallel prettier:**",
    "prettier:ts": "prettier --write \"src/**/*.ts?(x)\"",
    "prettier:styles": "prettier --write \"src/**/*.scss\"",
    "release:prepare": "npm version prerelease --preid=beta --no-git-tag-version ",
    "release:publish": "./support/release.sh",
    "postrelease:publish": "npm run deploy-storybook",
    "prestorybook": "npm run build",
    "storybook": "start-storybook -s ./dist",
    "prebuild-storybook": "npm run build",
    "build-storybook": "build-storybook -s ./dist -o .docs",
    "deploy-storybook": "storybook-to-ghpages",
    "deploy": "npm-run-all build deploy-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/calcite-components.git"
  },
  "dependencies": {
    "@a11y/focus-trap": "git+https://github.com/patrickarlt/focus-trap.git#conditional-define-build",
    "@popperjs/core": "^2.4.4",
    "@types/color": "3.0.1",
    "sortablejs": "1.10.2"
  },
  "devDependencies": {
    "@esri/calcite-base": "^1.2.0",
    "@esri/calcite-colors": "^2.1.0",
    "@esri/calcite-ui-icons": "^3.8.0",
    "@stencil/core": "1.17.2",
    "@stencil/eslint-plugin": "0.3.1",
    "@stencil/postcss": "1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/state-tunnel": "^1.0.1",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-centered": "^5.3.19",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/cli": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@storybook/storybook-deployer": "2.8.6",
    "@types/autoprefixer": "9.7.2",
    "@types/dedent": "0.7.0",
    "@types/git-semver-tags": "3.0.0",
    "@types/jest": "25.2.3",
    "@types/jest-axe": "3.5.0",
    "@types/lodash-es": "4.17.3",
    "@types/pify": "3.0.2",
    "@types/puppeteer": "1.20.2",
    "@types/rimraf": "3.0.0",
    "@types/sortablejs": "1.10.5",
    "@types/webpack": "4.41.21",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "autoprefixer": "9.8.5",
    "awesome-typescript-loader": "5.2.1",
    "axe-core": "^3.5.5",
    "babel-loader": "~8.1.0",
    "chalk": "4.1.0",
    "chokidar": "3.4.1",
    "color": "3.1.2",
    "concurrently": "5.2.0",
    "cpy-cli": "^3.1.1",
    "dedent": "0.7.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "gh-release": "^3.5.0",
    "git-semver-tags": "4.0.0",
    "github-release-from-cc-changelog": "2.2.0",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-axe": "^3.5.0",
    "jest-cli": "24.9.0",
    "lint-staged": "^10.2.11",
    "lodash-es": "^4.17.15",
    "npm-run-all": "4.1.5",
    "package": "1.0.1",
    "pify": "5.0.0",
    "posthtml": "0.13.1",
    "prettier": "^2.0.5",
    "puppeteer": "2.1.0",
    "rimraf": "3.0.2",
    "standard-version": "8.0.2",
    "storybook": "^5.3.19",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-stencil": "1.0.1",
    "updtr": "3.1.0",
    "workbox-build": "^5.1.3"
  },
  "license": "UNLICENSED"
}
